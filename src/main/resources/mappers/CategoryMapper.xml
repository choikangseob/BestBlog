<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ks.bestblog.repository.CategoryMybatisRepository">

        <select id="findById" resultType="com.ks.bestblog.dto.response.CategoryResponse">
            select *
            from category
            where id = ${id};


        </select>


</mapper>